.top {
	width: 100%;
	z-index: 2;
	position: fixed;
	top: 0;
	text-align: center;
	background-color: #FFFFFF;
}
.top input {
	width: 630rpx;
	border-radius: 30rpx;
	height: 58rpx;
	line-height: 58rpx;
	color: #696969;
	background: #f5f5f5 url(data:image/png;base64,) no-repeat left 37rpx center;
	background-size: 28rpx 28rpx;
	padding-left: 80rpx;
	display: inline-block;
	margin: 20rpx 0;
	text-align: left;
	font-size: 30rpx;
}
.swiper {
	height: 364rpx;
	margin-top: 98rpx;
}
.swiper image {
	height: 364rpx;
	width: 100%;
}
.form{
	border-top: 20rpx solid #e4e4e4;
}
.form .title{
	height: 100rpx;
	line-height: 100rpx;
	padding: 0 20rpx;
	color:#469ffb;
	font-size: 30rpx;
	border-bottom: 1rpx solid #e4e4e4;
}
.group{
	padding: 0 20rpx;
	line-height: 100rpx;
	border-bottom: 1rpx solid #e4e4e4;
}
.group .left{
	max-width: 550rpx;
}
.group textarea{
	max-width: 550rpx;
}
.group .desc{
	height: 500rpx;
}
.group image{
	width: 200rpx;
	height: 200rpx;
}
.group .text{
	width: 160rpx;
	text-align: left;
}
.group radio{
	-webkit-transform:scale(0.7);
	        transform:scale(0.7)
}
.group input{
	height: 100rpx;
	line-height: 100rpx;
	width: 550rpx;
}
.group button{
	vertical-align: middle;
}
.submit{
	text-align: center;
	margin: 30rpx 0;
}

