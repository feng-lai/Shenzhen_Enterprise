.entry {
	background-color: #ffffff;
	padding-top: 10rpx;
	border-top: 1rpx solid #f5f5f5;
	padding-bottom: 100rpx;
}
.entry>.title {
	padding: 0 20rpx;
	color: #045fa2;
	font-size: 24rpx;
	height: 50rpx;
	line-height: 50rpx;
}
.entry>.row {
	background-color: #fff;
	border-radius: 15rpx;
	display: block;
	border-bottom: 20rpx solid #f5f5f5;
}
.entry>.row:last-child {
	border-bottom: 0;
}
.entry>.row>.user{
	border-bottom: 1rpx solid #f5f5f5;
	height: 100rpx;
	padding: 20rpx;
}
.entry>.row>.user image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 100rpx;
	float: left;
}
.entry>.row>.user .right{
	margin-left: 50rpx;
}
.entry>.row>.user .right::after{
	border: none;
}
.entry>.row>.user button{
	background-color: #ffffff;
}
.entry>.row>.user .text{
	line-height: 50rpx;
	margin-left: 20rpx;
}
.entry>.row>.user .right{
	height: 100rpx;
	line-height: 100rpx;
}
.entry>.row>.content{
	padding: 10rpx 20rpx;
	display: flex;
}
.entry>.row>.content>.img {
	width: 135rpx;
	height: 135rpx;
	text-align: center;
	margin-right: 10rpx;
}
.entry>.row>.content>.left>image {
	max-width: 135rpx;
	height: 135rpx;
	border-radius: 10rpx;
}
.entry>.row>.content>.left>.desc {
	width: 550rpx;
	font-size: 20rpx;
	color: #999999;
	line-height: 35rpx;
	height: 70rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 2;
	overflow: hidden;
}
.entry>.row>.content>.left>.title {
	color: #000;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	width: 550rpx;
}
